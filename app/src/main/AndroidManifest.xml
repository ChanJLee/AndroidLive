<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"          package="com.wenyu.ylive">    <uses-permission android:name="android.permission.INTERNET"/>    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>    <uses-permission android:name="android.permission.CAMERA"/>    <uses-permission android:name="android.permission.FLASHLIGHT"/>    <uses-permission android:name="android.permission.RECORD_AUDIO"/>    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS"/>    <uses-permission android:name="android.permission.WAKE_LOCK"/>    <uses-feature android:name="android.hardware.camera"/>    <uses-feature android:name="android.hardware.autofocus"/>    <application        android:name=".base.YLiveApplication"        android:allowBackup="true"        android:icon="@mipmap/ic_launcher"        android:label="@string/app_name"        android:supportsRtl="true"        android:theme="@style/AppTheme">        <activity            android:name=".biz.splash.SplashActivity"            android:configChanges="orientation|keyboardHidden|keyboard"            android:screenOrientation="portrait"            android:theme="@style/AppTheme.NoActionBar">            <intent-filter>                <action android:name="android.intent.action.MAIN"/>                <category android:name="android.intent.category.LAUNCHER"/>            </intent-filter>        </activity>        <activity            android:name=".biz.home.thiz.HomeActivity"            android:configChanges="orientation|keyboardHidden|keyboard"            android:launchMode="singleTop"            android:screenOrientation="portrait"            android:theme="@style/AppTheme.NoActionBar"            android:windowSoftInputMode="adjustNothing|stateAlwaysHidden">            <intent-filter>                <action android:name="com.wenyu.backhome"/>                <data                    android:host="${applicationId}"                    android:scheme="ylive"/>                <category android:name="android.intent.category.DEFAULT"/>            </intent-filter>        </activity>        <activity            android:name=".biz.profile.LoginActivity"            android:label="@string/title_activity_login">        </activity>        <activity android:name=".test.XMPPActivity">        </activity>        <activity android:name=".test.DanmaActivity">        </activity>        <activity android:name=".test.RTMPActivity">        </activity>        <activity android:name=".biz.live.LiveActivity">        </activity>        <activity android:name=".biz.video.VideoActivity">        </activity>    </application></manifest>